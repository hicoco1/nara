@CHARSET "UTF-8";

/* Nanum Gothic */
@font-face {
	font-family:'ng';
	src:url('./font/NanumGothic-Regular.eot');	
	src:local(※),
		url('/font/NanumGothic-Regular.eot?#iefix') format('embedded-opentype'),
		url('/font/NanumGothic-Regular.woff') format('woff'),		
		url('/font/NanumGothic-Regular.ttf') format('truetype');
	font-weight: normal;
	font-style: normal;
}

/* Nanum Gothic Bold */
@font-face {
	font-family:'ng_b';
	src:url('./font/NanumGothic-Bold.eot');	
	src:local(※),
		url('/font/NanumGothic-Bold.eot?#iefix') format('embedded-opentype'),
		url('/font/NanumGothic-Bold.woff') format('woff'),		
		url('/font/NanumGothic-Bold.ttf') format('truetype');
	font-weight: normal;
	font-style: normal;
}

*    	  	{margin:0; padding:0;}
body		{background-color:#fff; color:#666; line-height:18px; font-size:12px;}

ul, ol, li	{list-style:none; }

address, cite, em	{font-style:normal;}

img			{border: none; vertical-align: top; border: 0px;}

/* Link Style */
a:link		{text-decoration:none; color:inherit;}
a:visited	{text-decoration:none; color:inherit;}
a:active	{text-decoration:none; color:inherit;}
a:hover		{text-decoration:none; color:inherit;}



.pop_title_area {padding: 12px 0px 12px 10px ;  border-bottom: solid 3px #e02d2d; background-color:#fff; overflow:hidden; }
.pop_title_area .pop_title {float: left; padding-left: 16px;  background:url(/images/popup/bul1.gif) 0 center no-repeat; font-size:18px; line-height:20px; color:#222222; line-height:32px; margin-right: 10px;}
.pop_title_area select {width: 200px; border: solid 1px #dfdfdf; height: 32px; vertical-align:middle; line-height:28px; font-size:14px; font-family:'ng_b'; }
.pop_contents {padding:10px 0 0 10px; overflow:hidden; }

.pop_box {float: left; width: 474px; margin-right: 10px; margin-bottom: 10px;  background-color:#fff; overflow:hidden; position:relative; 
	-moz-border-radius: 10px; 
	-webkit-border-radius: 10px;
	border-radius: 10px;

	-webkit-box-shadow: 2px 2px 1px 0px rgba(0,0,0,0.3);
	-moz-box-shadow: 2px 2px 1px 0px rgba(0,0,0,0.3);
	box-shadow: 2px 2px 1px 0px rgba(0,0,0,0.3);
}
.pop_box.type1 { width: 474px;}
.pop_box.type2 { width: 678px;}
.pop_box.type3 { width: 958px;}
.pop_box.type4 { width: 945px;}

.pop_box_title {padding: 10px 20px; border-bottom: solid 2px #6c6c6c; position:relative; font-size:14px; color:#222222; line-height:17px; }
.pop_box_title.noline {border-bottom: solid 0px #fff;}
.pop_box_title .amount {padding: 0 5px; display:inline-block; font-size:12px; font-size:14px; color:#fff; line-height:17px; background-color:#e02d2d;}

