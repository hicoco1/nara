.pop_box_state table {}
.pop_box_state table td {padding: 7px 0 7px 32px; text-align:letf; border-left: solid 1px #d8d8d8;}
.pop_box_state table td:first-child {border-left: solid 0px #fff;}

.pop_box_hw_state table {}
.pop_box_hw_state table td {padding: 7px 0 7px 32px; text-align:left; border-left: solid 1px #d8d8d8;}
.pop_box_hw_state table td:first-child {border-left: solid 0px #fff;}

.pop_box_outnode_state table {vertical-align: middle;}
.pop_box_outnode_state table td {padding: 7px 0 7px 20px; text-align:center; vertical-align:middle; border-left: solid 1px #d8d8d8;}
.pop_box_outnode_state table td:first-child {border-left: solid 0px #fff;}

.pop_box_state .graph {display:inline-block; width: 99px; height: 17px; margin-left: 5px; position:relative; background-color:#bebebe; vertical-align:middle; }
.pop_box_state .graph .back {position:absolute; left:0px; top:0px; }
.pop_box_state .graph strong {display:block; height: 17px; background-color:#404040;}
.pop_box_state .graph .txt {position:absolute; right:0px; top:-2px; font-size:11px; line-height:13px; color:#131313; font-family:'ng_b';}

.btn_style1 {display:inline-block; padding: 0 5px;  height: 25px; line-height:25px; font-size:12px; color:#222222 !important; border: solid 1px #9f9f9f; background-color:#ffffff; font-family:'ng_b';
