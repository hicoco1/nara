.pop_box_server_conts {padding-bottom: 10px; }
.pop_box_server_conts table {}
.pop_box_server_conts table th {padding: 5px 0; background-color:#505050; border-left: solid 1px #2d2d2d; font-size:12px; line-height:16px; color:#fff; text-align:center; }
.pop_box_server_conts table th:first-child {border-left: solid 0px #fff;}
.pop_box_server_conts table td {padding: 5px; text-align:center; background-color:#ffffff; font-size:11px; color:#666666; line-height:16px; }
.pop_box_server_conts table td.l {text-align:left; }
.pop_box_server_conts table td.r {text-align:right; }

.pop_box_server_conts table tr:nth-child(odd) td {background-color:#f9f9f9;}


